[MITM]
{{{禁用MITM}}} =  %APPEND% argo.blued.cn,<ip-address>,burn-chatfiles.bldimg.com,-blued.irisdt.cn,-120.53.131.149,-live-im.blued.cn,-h*.blued.cn,-95.161.76.101,-101.23.128.20,-129.226.78.144,-112.28.199.148,-120.53.131.149

[Script]
{{{Eric捕获密照}}} = type=http-request,pattern=^https:\/\/burn-chatfiles\.bldimg\.com\/burn_chatfiles\/(videos|users)\/.*(mp4|jpg).*,script-path=https://raw.githubusercontent.com/Alex0510/Eric/master/surge/Script/blued-pic.js,requires-body=0,max-size=0,debug=0
{{{Eric禁用定位}}} = type=http-request,pattern=https://(argo.blued.cn|\b((25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.){3}(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\b)/(blued|users|users\?extra_info=|ticktocks/users|groups/nearby).*,requires-body=0,script-path=https://raw.githubusercontent.com/Alex0510/Eric/master/surge/Script/blued_ip.js
{{{Eric禁用在线}}} = type=http-request,pattern=https://(argo.blued.cn|\b((25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.){3}(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\b)/(blued|users|users\?extra_info=).*,requires-body=0,script-path=https://raw.githubusercontent.com/Alex0510/Eric/master/surge/Script/Blued.js
blued = type=http-response,pattern=https://(argo.blued.cn|\b((25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.){3}(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\b)/users/.*,requires-body=1,max-size=0,binary-body-mode=0,script-path=https://raw.githubusercontent.com/Alex0510/Eric/master/surge/Script/blued.js,script-update-interval=0
验证密码 = type=generic,timeout=10,script-path=https://raw.githubusercontent.com/Alex0510/Eric/master/surge/Script/bb.js

